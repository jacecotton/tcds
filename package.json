{
  "name": "@txch/tcds",
  "version": "0.60.19",
  "description": "Texas Children's Design System",
  "type": "module",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "build:scripts": "gulp scripts",
    "build:styles": "gulp styles",
    "build:icons": "gulp icons",
    "build:images": "gulp images",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:styles": "stylelint 'assets/**/*.scss'",
    "lint:scripts": "eslint 'assets/**/*.js'",
    "fix": "npm run fix:styles && npm run fix:scripts",
    "fix:styles": "stylelint 'assets/**/*.scss' --fix",
    "fix:scripts": "eslint 'assets/**/*.js' --fix",
    "version": "cat package.json | grep -e '^  \"version\": ' | awk '{print $2}' | sed 's/[\",]//g'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacecotton/tcds.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jacecotton/tcds/issues"
  },
  "homepage": "https://github.com/jacecotton/tcds#readme",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.12.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^8.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "sass": "^1.49.9",
    "stylelint": "^14.6.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "webpack-stream": "^7.0.0"
  }
}
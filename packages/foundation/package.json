{
  "name": "@txch/foundation",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "generate:config": "node scripts/config.js",
    "generate:icons": "node scripts/icons.js",
    "build:scss": "sass --load-path=../../node_modules --style=compressed --source-map src:dist",
    "prebuild": "npm run clean && npm run generate:config && npm run generate:icons",
    "build": "npm run prebuild && npm run build:scss"
  },
  "devDependencies": {
    "@txch/tokens": "^3.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2"
  }
}

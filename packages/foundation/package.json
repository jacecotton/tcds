{
  "name": "@txch/foundation",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild:config": "node build/config.js",
    "prebuild:icons": "node build/icons.js",
    "prebuild": "npm run clean && npm run prebuild:config && npm run prebuild:icons",
    "build:scss": "sass --load-path=../../node_modules --style=compressed --source-map src:dist",
    "build": "npm run prebuild && npm run build:scss"
  },
  "devDependencies": {
    "@txch/tokens": "^3.0.0",
    "@txch/media": "^3.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2"
  }
}

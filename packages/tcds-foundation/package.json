{
  "name": "@txch/tcds-foundation",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "sass --load-path=../../node_modules --style=compressed --source-map src:dist",
    "dev": "sass --load-path=../../node_modules --style=compressed --source-map --watch src:dist",
    "check-build-gzip": "gzip -c dist/foundation.css | wc -c"
  },
  "devDependencies": {
    "@txch/tcds-identity": "^3.0.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-replace": "^2.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2"
  }
}

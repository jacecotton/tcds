{% embed "@tcds/components/tabs/tabs-container.html.twig" with {
  modifiers: modifiers,
  custom_classes: custom_classes,
  custom_attributes: custom_attributes,
} %}
  {% block content %}
    {% for tab in tabs %}
      {{ include("@tcds/components/tabs/tabs-item.html.twig", {
        heading: tab.heading,
        content: tab.content,
      }, with_context = false) }}
    {% endfor %}
  {% endblock %}
{% endembed %}
